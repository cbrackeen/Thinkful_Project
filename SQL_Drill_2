SELECT 
  Date, 
  max(MaxTemperatureF), 
  zip
FROM 
  weather
Date: "2015-11-17"	Temp: "134"	Zip: "94063"

SELECT 
  start_station, 
    count(*)
FROM 
  trips
GROUP BY 
  start_station

SELECT 
	trip_id, 
	min(duration)
FROM 
	trips
	
  SELECT 
	AVG(duration),
	end_station
FROM 
	trips
GROUP BY
	end_station
	
